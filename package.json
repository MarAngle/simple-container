{
  "name": "simple-container",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "pre": "yarn install --registry https://registry.yarnpkg.com || npm install --registry https://registry.npm.taobao.org",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "ant-design-vue": "^4.0.8",
    "complex-utils": "2.1.0 - 2.1.99",
    "complex-plugin": "4.1.0 - 4.1.99",
    "complex-request-axios": "0.2.0 - 0.2.99",
    "complex-data": "4.1.7 - 4.1.99",
    "complex-component": "4.1.4 - 4.1.99",
    "core-js": "^3.8.3",
    "vue": "^3.2.13",
    "vue-router": "^4.0.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "less": "^4.0.0",
    "less-loader": "^8.0.0",
    "lint-staged": "^11.1.2",
    "typescript": "^5.2.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/typescript/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {
      "@typescript-eslint/no-this-alias": 0,
      "@typescript-eslint/no-non-null-assertion": 0
    }
  },
  "browserslist": [
    "Chrome > 105"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": "vue-cli-service lint"
  }
}
